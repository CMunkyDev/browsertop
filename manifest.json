{
  "manifest_version": 2,
  "name": "Browsertop Tab",
  "version": "0.1",
  "version_name": "0.1 alpha",

  "description": "A simulated desktop experience for your browser.",

  "author": "CMunky",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "history",
    "idle",
    "management",
    "notifications",
    "tabs",
    "storage"
  ]
}
